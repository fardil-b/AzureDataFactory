{
	"name": "Parquet1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"SinkFileName": {
				"type": "String"
			}
		},
		"folder": {
			"name": "old dataset"
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@{dataset().SinkFileName}.parquet",
					"type": "Expression"
				},
				"fileSystem": "lego"
			},
			"compressionCodec": "snappy"
		},
		"schema": [
			{
				"name": "id",
				"type": "UTF8"
			},
			{
				"name": "name",
				"type": "UTF8"
			},
			{
				"name": "parent_id",
				"type": "UTF8"
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}